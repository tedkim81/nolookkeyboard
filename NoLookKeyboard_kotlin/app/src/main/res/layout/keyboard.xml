<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:background="@drawable/bg_keyboard"
	android:padding="5dip"
	>
	<com.teuskim.nlkeyboard.KeySetView
		android:id="@+id/keyset_left_top"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentLeft="true"
		android:layout_alignParentTop="true"
		/>
	<com.teuskim.nlkeyboard.KeySetView
		android:id="@+id/keyset_mid_top"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerHorizontal="true"
		android:layout_alignParentTop="true"
		/>
	<com.teuskim.nlkeyboard.KeySetView
		android:id="@+id/keyset_right_top"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentRight="true"
		android:layout_alignParentTop="true"
		/>
	<com.teuskim.nlkeyboard.KeySetView
		android:id="@+id/keyset_left_mid"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentLeft="true"
		android:layout_centerVertical="true"
		/>
	<com.teuskim.nlkeyboard.KeySetView
		android:id="@+id/keyset_right_mid"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentRight="true"
		android:layout_centerVertical="true"
		/>
	<com.teuskim.nlkeyboard.KeySetView
		android:id="@+id/keyset_left_bot"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentLeft="true"
		android:layout_alignParentBottom="true"
		/>
	<com.teuskim.nlkeyboard.KeySetView
		android:id="@+id/keyset_mid_bot"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerHorizontal="true"
		android:layout_alignParentBottom="true"
		/>
	<com.teuskim.nlkeyboard.KeySetView
		android:id="@+id/keyset_right_bot"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentRight="true"
		android:layout_alignParentBottom="true"
		/>
	<ImageView
		android:id="@+id/keyboard_center_x"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerInParent="true"
		android:src="@drawable/keyboard_center_x"
		/>
	<ImageView
		android:id="@+id/keyboard_center_plus"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerInParent="true"
		android:src="@drawable/keyboard_center_plus"
		/>
	<ImageView
		android:id="@+id/keyboard_bg_x"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:background="@drawable/keyboard_center_x"
		android:visibility="gone"
		/>
	<ImageView
		android:id="@+id/keyboard_bg_plus"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:background="@drawable/keyboard_center_plus"
		android:visibility="gone"
		/>
</RelativeLayout>
