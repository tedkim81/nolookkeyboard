<Keyboard 
	xmlns:android="http://schemas.android.com/apk/res/android"
	>

	<KeySet><!-- left top -->
		<Key android:codes="1" android:keyLabel="\u3132"/><!-- ㄲ -->
		<Key android:codes="2" android:keyLabel="\u3134"/><!-- ㄴ -->
		<Key android:codes="15" android:keyLabel="\u314b"/><!-- ㅋ -->
	</KeySet>
	
	<KeySet><!-- mid top -->
		<Key android:codes="-1" android:keyLabel="shift" android:keyIcon="@drawable/key_shift"/>
		<Key android:codes="4" android:keyLabel="\u3138"/><!-- ㄸ -->
		<Key android:codes="5" android:keyLabel="\u3139"/><!-- ㄹ -->
		<Key android:codes="16" android:keyLabel="\u314c"/><!-- ㅌ -->
	</KeySet>
	
	<KeySet><!-- right top -->
		<Key android:codes="6" android:keyLabel="\u3141"/><!-- ㅁ -->
		<Key android:codes="8" android:keyLabel="\u3143"/><!-- ㅃ -->
		<Key android:codes="17" android:keyLabel="\u314d"/><!-- ㅍ -->
	</KeySet>
	
	<KeySet><!-- left mid -->
		<Key android:codes="-5" android:keyLabel="backspace" android:keyIcon="@drawable/key_backspace"/>
		<Key android:codes="11" android:keyLabel="\u3147"/><!-- ㅇ -->
		<Key android:codes="18" android:keyLabel="\u314e"/><!-- ㅎ -->
	</KeySet>
		
	<KeySet><!-- right mid -->
		<Key android:codes="-2" android:keyLabel="spacebar" android:keyIcon="@drawable/key_space"/>
		<Key android:codes="10" android:keyLabel="\u3146"/><!-- ㅆ -->
		<Key android:codes="13" android:keyLabel="\u3149"/><!-- ㅉ -->
		<Key android:codes="14" android:keyLabel="\u314a"/><!-- ㅊ -->
	</KeySet>
	
	<KeySet><!-- left bot -->	
		<Key android:codes="27" android:keyLabel="\u3157"/><!-- ㅗ -->
		<Key android:codes="29" android:keyLabel="\u315c"/><!-- ㅜ -->
		<Key android:codes="28" android:keyLabel="\u315b"/><!-- ㅛ -->
		<Key android:codes="30" android:keyLabel="\u3160"/><!-- ㅠ -->
	</KeySet>
	
	<KeySet><!-- mid bot -->
		<Key android:codes="-3" android:keyLabel="enter" android:keyIcon="@drawable/key_enter"/>
		<Key android:codes="32" android:keyLabel="\u3163"/><!-- ㅣ -->
		<Key android:codes="31" android:keyLabel="\u3161"/><!-- ㅡ -->
	</KeySet>
		
	<KeySet><!-- right bot -->
		<Key android:codes="20" android:keyLabel="\u3150"/><!-- ㅐ -->
		<Key android:codes="24" android:keyLabel="\u3154"/><!-- ㅔ -->		
		<Key android:codes="22" android:keyLabel="\u3152"/><!-- ㅒ -->
		<Key android:codes="26" android:keyLabel="\u3156"/><!-- ㅖ -->
	</KeySet>
		
</Keyboard>
    